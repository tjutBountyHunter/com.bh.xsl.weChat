<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xsl.wechat.mapper.AuthenticationMapper">


    <update id="AuthenticationUser" parameterType="com.xsl.wechat.vo.AuthenticationReqVo">
        UPDATE xsl_user set name=#{name},phone=#{phoneNumber},state=1 WHERE userId=#{userId}
    </update>
</mapper>